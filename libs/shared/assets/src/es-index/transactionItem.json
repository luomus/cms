{
  "settings": {
    "number_of_shards": 3,
    "index.mapping.ignore_malformed": true,
    "index.mapping.total_fields.limit": 10000,
    "index.mapping.nested_objects.limit": 100000
  },
  "mappings": {
    "properties": {
      "@context": {
        "type": "keyword"
      },
      "id": {
        "type": "keyword"
      },
      "@type": {
        "type": "keyword"
      },
      "collectionID": {
        "type": "keyword",
        "ignore_above": 200
      },
      "itemCount": {
        "type": "integer"
      },
      "itemID": {
        "type": "keyword",
        "ignore_above": 200
      },
      "itemLabel": {
        "type": "keyword",
        "ignore_above": 200
      },
      "itemType": {
        "type": "keyword",
        "ignore_above": 100
      },
      "notes": {
        "type": "keyword",
        "ignore_above": 200
      },
      "status": {
        "type": "keyword",
        "ignore_above": 100
      },
      "_meta": {
        "properties": {
          "created": {
            "type": "date"
          },
          "edited": {
            "type": "date"
          }
        }
      }
    }
  }
}