{
  "settings": {
    "number_of_shards": 1,
    "index.mapping.ignore_malformed": true,
    "index.mapping.total_fields.limit": 10000,
    "index.mapping.nested_objects.limit": 100000
  },
  "mappings": {
    "properties": {
      "@context": {
        "type": "keyword"
      },
      "id": {
        "type": "keyword"
      },
      "@type": {
        "type": "keyword"
      },
      "congruent": {
        "type": "keyword"
      },
      "excludes": {
        "type": "keyword"
      },
      "includedIn": {
        "type": "keyword"
      },
      "overlaps": {
        "type": "keyword"
      },
      "_meta": {
        "properties": {
          "created": {
            "type": "date"
          },
          "edited": {
            "type": "date"
          }
        }
      }
    }
  }
}